{
    "manifest_version": 3,
    "name": "Google Domain Scraper",
    "version": "1.0",
    "description": "Search Google and extract domain names from search results",
    "permissions": [
      "activeTab",
      "storage",
      "downloads",
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "https://www.google.com/*",
      "https://www.google.co.uk/*",
      "https://www.google.ca/*",
      "https://www.google.com.au/*",
      "https://www.google.de/*",
      "https://www.google.fr/*",
      "https://www.google.co.jp/*",
      "https://www.google.co.in/*",
      "https://www.google.com.br/*",
      "https://www.google.cn/*",
      "https://www.google.es/*",
      "https://www.google.it/*",
      "https://www.google.com.mx/*",
      "https://www.google.ru/*",
      "https://www.google.co.kr/*",
      "https://www.google.nl/*",
      "https://www.google.se/*",
      "https://www.google.no/*",
      "https://www.google.dk/*",
      "https://www.google.fi/*",
      "https://www.google.ch/*",
      "https://www.google.at/*",
      "https://www.google.be/*",
      "https://www.google.pl/*",
      "https://www.google.com.tr/*",
      "https://www.google.co.za/*",
      "https://www.google.com.ar/*",
      "https://www.google.cl/*",
      "https://www.google.com.co/*",
      "https://www.google.com.pe/*",
      "https://www.google.co.ve/*",
      "https://www.google.co.nz/*",
      "https://www.google.ie/*",
      "https://www.google.pt/*",
      "https://www.google.gr/*",
      "https://www.google.cz/*",
      "https://www.google.hu/*",
      "https://www.google.ro/*",
      "https://www.google.bg/*",
      "https://www.google.hr/*",
      "https://www.google.si/*",
      "https://www.google.sk/*",
      "https://www.google.ee/*",
      "https://www.google.lv/*",
      "https://www.google.lt/*",
      "https://www.google.lu/*",
      "https://www.google.is/*",
      "https://www.google.com.mt/*",
      "https://www.google.com.cy/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.google.com/search*",
          "https://www.google.co.uk/search*",
          "https://www.google.ca/search*",
          "https://www.google.com.au/search*",
          "https://www.google.de/search*",
          "https://www.google.fr/search*",
          "https://www.google.co.jp/search*",
          "https://www.google.co.in/search*",
          "https://www.google.com.br/search*",
          "https://www.google.cn/search*",
          "https://www.google.es/search*",
          "https://www.google.it/search*",
          "https://www.google.com.mx/search*",
          "https://www.google.ru/search*",
          "https://www.google.co.kr/search*",
          "https://www.google.nl/search*",
          "https://www.google.se/search*",
          "https://www.google.no/search*",
          "https://www.google.dk/search*",
          "https://www.google.fi/search*",
          "https://www.google.ch/search*",
          "https://www.google.at/search*",
          "https://www.google.be/search*",
          "https://www.google.pl/search*",
          "https://www.google.com.tr/search*",
          "https://www.google.co.za/search*",
          "https://www.google.com.ar/search*",
          "https://www.google.cl/search*",
          "https://www.google.com.co/search*",
          "https://www.google.com.pe/search*",
          "https://www.google.co.ve/search*",
          "https://www.google.co.nz/search*",
          "https://www.google.ie/search*",
          "https://www.google.pt/search*",
          "https://www.google.gr/search*",
          "https://www.google.cz/search*",
          "https://www.google.hu/search*",
          "https://www.google.ro/search*",
          "https://www.google.bg/search*",
          "https://www.google.hr/search*",
          "https://www.google.si/search*",
          "https://www.google.sk/search*",
          "https://www.google.ee/search*",
          "https://www.google.lv/search*",
          "https://www.google.lt/search*",
          "https://www.google.lu/search*",
          "https://www.google.is/search*",
          "https://www.google.com.mt/search*",
          "https://www.google.com.cy/search*"
        ],
        "js": ["content.js"],
        "run_at": "document_idle",
        "all_frames": false,
        "match_about_blank": false
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["countries.csv"],
        "matches": ["<all_urls>"]
      }
    ]
  }